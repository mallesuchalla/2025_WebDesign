    }
    function ltrim(str, chars) {
        chars = chars || "\\s";
        return str.replace(new RegExp("^[" + chars + "]+", "g"), "");
    }
    function rtrim(str, chars) {
        chars = chars || "\\s";
        return str.replace(new RegExp("[" + chars + "]+$", "g"), "");
    }
    function joindomain()
    {

        var domainstr = '';
        var domainname = document.getElementById('bulkdomains').value;
        var rep_space = domainname.replace(/\s/g, "\n");
