            $.ajax({
                url: "/otp-doverification-email",  // Correct URL
                data: formData,
                type: "post",
                success: function(response) {
                    console.log("hite:" + response);
                    $("#verifyotpbtn").text("Verify OTP");
                    if (response && response.status == 1) {
                        localStorage.setItem('popupshow', '11'); // Store popupshow value
                        $("#signup-form").submit();
                    } else {
                        $("#verifyotpalert").show();
                    }
                },error: function(xhr, status, error) {
                // Handle any errors from the AJAX request
                    console.log(xhr);
                    console.log(status);
                    console.log(error);
                $("#verifyotpalert").show();  // Show alert if there's a request failure
                $("#verifyotpbtnss").text("Verify OTP");  // Revert button text
            }
            });
            return false; // Prevent form from submitting normally
